{
    "name": "asantibanez/livewire-resource-time-grid",
    "description": "Laravel Livewire resource time grid component",
    "keywords": [
        "asantibanez",
        "livewire-resource-time-grid"
    ],
    "homepage": "https://github.com/asantibanez/livewire-resource-time-grid",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andrés Santibáñez",
            "email": "santibanez.andres@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0",
        "livewire/livewire": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Asantibanez\\LivewireResourceTimeGrid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asantibanez\\LivewireResourceTimeGrid\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asantibanez\\LivewireResourceTimeGrid\\LivewireResourceTimeGridServiceProvider"
            ]
        }
    }
}
